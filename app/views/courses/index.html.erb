<h1>Listing courses</h1>

<table>
  <thead>
    <tr>
      <th>Exam</th>
      <th>Data</th>
      <th>Report</th>
      <th>P name</th>
      <th>Name</th>
      <th>Course_id</th>
      <th>Attendance</th>
      <th>Start time</th>
      <th>Day week</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.exam %></td>
        <td><%= course.data %></td>
        <td><%= course.report %></td>
        <td><%= course.p_name %></td>
        <td><%= course.name %></td>
        <td><%= course.course_id %></td>
        <td><%= course.attendance %></td>
        <td><%= course.start_time %></td>
        <td><%= course.day_week %></td>
        <td><%= link_to 'Show', course %></td>
        <td><%= link_to 'Edit', edit_course_path(course) %></td>
        <td><%= link_to 'Destroy', course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course', new_course_path %>
